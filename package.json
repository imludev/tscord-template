{
  "name": "tscord-template",
  "version": "1.0.0",
  "description": "A fully-featured discord bots template written in Typescript, intended to provide a framework that's easy to use, extend and modify",
  "license": "MIT",
  "main": "build/main.js",
  "scripts": {
    "prebuild": "rimraf build/",
    "build": "npm run prebuild && tsc --project tsconfig.json && npm run postbuild",
    "postbuild": "tsc-alias -p tsconfig.json && npm run copy:static",
    "build:changelog": "npx @discordx/changelog --root=src",
    "build:serve": "npm run build && npm run serve",
    "copy:static": "ncp ./assets ./build/assets",
    "serve": "cross-env NODE_ENV=production node build/src/main.js",
    "start": "cross-env NODE_ENV=production node --loader ts-node/esm/transpile-only src/main.ts",
    "dev": "npm-run-all --parallel dev:*",
    "dev:watch": "cross-env NODE_ENV=development nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts",
    "dev:i18n": "typesafe-i18n",
    "test": "nodemon --exec node --loader ts-node/esm/transpile-only tests/main.ts",
    "migration:create": "npx mikro-orm migration:create",
    "migration:up": "npx mikro-orm migration:up",
    "migration:down": "npx mikro-orm migration:down",
    "plop": "plop --plopfile ./cli/plopfile.js"
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@discordx/koa": "^1.1.0",
    "@discordx/music": "^4.1.0",
    "@discordx/pagination": "^3.0.0",
    "@discordx/utilities": "^5.0.0",
    "@koa/router": "^10.1.1",
    "@mikro-orm/cli": "^5.1.4",
    "@mikro-orm/core": "^5.1.4",
    "@mikro-orm/migrations": "^5.1.4",
    "@mikro-orm/mysql": "^5.2.1",
    "@mikro-orm/postgresql": "^5.2.1",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@mikro-orm/sqlite": "^5.1.4",
    "axios": "^0.27.2",
    "boxen": "^5.1.2",
    "case": "^1.6.3",
    "chalk": "^4.1.2",
    "cron": "^2.0.0",
    "cron-validator": "^1.3.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.3",
    "discord-api-types": "^0.36.2",
    "discord-oauth2": "^2.10.0",
    "discord.js": "^14.0.3",
    "discordx": "^10.0.0",
    "dotenv": "^16.0.1",
    "fs": "^0.0.1-security",
    "http-status-codes": "^2.2.0",
    "image-hash": "^5.3.1",
    "imgur": "^2.2.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-context-validator": "^1.0.0",
    "node-os-utils": "^1.3.7",
    "oneline": "^1.0.3",
    "ora": "^5.4.1",
    "pidusage": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rxeta": "^1.1.2",
    "saveqlite": "^1.1.2",
    "socket.io-client": "^4.5.1",
    "stacktrace-parser": "^0.1.10",
    "tsyringe": "^4.6.0",
    "typesafe-i18n": "^5.4.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.5.0",
    "@types/cron": "^2.0.0",
    "@types/dateformat": "^5.0.0",
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "@types/koa-bodyparser": "^4.3.7",
    "@types/node": "^17.0.33",
    "@types/node-os-utils": "^1.3.0",
    "@types/pidusage": "^2.0.2",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "plop": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.7",
    "tsconfig-paths": "^4.0.0",
    "typescript": "~4.6.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config.ts",
      "./build/mikro-orm.config.js"
    ]
  }
}
